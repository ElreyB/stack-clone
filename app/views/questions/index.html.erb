

<% if current_user %>
  <h1>Questions</h1>
  <% if @questions.any? %>
    <ul>
      <%@questions.each do |question| %>
        <li>
          <%= link_to question.title, question_path(question) %>
        </li>
      <% end %>
    </ul>
  <% else %>
    <p>Currently no Questions have been ask.</p>
  <% end %>
<% else %>
  <h1>Please sign in to see content!!!!</h1>
<% end %>
